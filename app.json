{
  "expo": {
    "name": "AllTrailsClone",
    "slug": "AllTrailsClone",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.justinkusz.mytrails",
      "supportsTablet": true
    },
    "android": {
      "package": "com.justinkusz.mytrails"
    },
    "androidStatusBar": {
      "backgroundColor": "#006400"
    }
  }
}